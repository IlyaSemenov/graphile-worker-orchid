{
  "name": "graphile-worker-orchid",
  "type": "module",
  "version": "1.2.0",
  "packageManager": "pnpm@9.8.0",
  "description": "Use graphile-worker with orchid-orm",
  "author": "Ilya Semenov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IlyaSemenov/graphile-worker-orchid"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "!src/**/*.test.*",
    "dist",
    "src"
  ],
  "scripts": {
    "prepare": "husky",
    "prepublishOnly": "pnpm build",
    "build": "tsup",
    "lint": "eslint --fix .",
    "test": "vitest run"
  },
  "peerDependencies": {
    "graphile-worker": ">=0.12",
    "orchid-orm": "^1.12.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@ilyasemenov/eslint-config": "^1.4.2",
    "eslint": "^9.36.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.1",
    "tsconfig-vite-node": "^1.1.2",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5"
  }
}
